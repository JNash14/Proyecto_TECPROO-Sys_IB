package Clases;

public class Producto {
private String nomp,cate,cod,preven, precos, cant;

public Producto(String nomp, String cate, String cod, String preven, String precos, String cant) {
	this.nomp = nomp;
	this.cate = cate;
	this.cod = cod;
	this.preven = preven;
	this.precos = precos;
	this.cant = cant;
}

public String getNomp() {
	return nomp;
}

public void setNomp(String nomp) {
	this.nomp = nomp;
}

public String getCate() {
	return cate;
}

public void setCate(String cate) {
	this.cate = cate;
}

public String getCod() {
	return cod;
}

public void setCod(String cod) {
	this.cod = cod;
}

public String getPreven() {
	return preven;
}

public void setPreven(String preven) {
	this.preven = preven;
}

public String getPrecos() {
	return precos;
}

public void setPrecos(String precos) {
	this.precos = precos;
}

public String getCant() {
	return cant;
}

public void setCant(String cant) {
	this.cant = cant;
}



}
